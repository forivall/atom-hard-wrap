{
  "name": "hard-wrap",
  "version": "1.7.1",
  "description": "Hard wraps (aka autoflow) your text; works with multi-wrap-guide",
  "keywords": [
    "autoflow",
    "wrap"
  ],
  "license": "MIT",
  "main": "./lib/hard-wrap",
  "repository": "https://github.com/forivall/atom-hard-wrap",
  "babel": {
    "babelrc": false,
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "sourceMaps": "inline"
  },
  "dependencies": {
    "escape-regex-string": "^1.0.6",
    "repeating": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^8.2.1",
    "eslint-config-mxo": "^0.5.4",
    "xo": "^0.18.2"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationCommands": {
    "atom-text-editor": "hard-wrap:reflow-selection"
  },
  "xo": {
    "extends": "mxo/ava",
    "parser": "babel-eslint",
    "globals": [
      "atom"
    ]
  }
}
